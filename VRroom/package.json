{
  "name": "VR-Cinema-Website-with-React",
  "version": "1.0.0",
  "description": "Virtual Reality Booking System for cinemas",
  "dependencies": {
    "aframe": "^0.2.0",
    "aframe-core": "^0.1.6",
    "aframe-extras": "^1.16.0",
    "aframe-fps-look-component": "^0.1.1",
    "aframe-text-component": "^0.3.0",
    "aframe-react": "^1.2.0",
    "babel-polyfill": "^6.3.14",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "tween": "^0.9.0"
  },
  "scripts": {
    "build": "webpack",
    "preghpages": "npm run build && rm -rf gh-pages && cp -r src gh-pages && cp index.html build/bundle.js gh-pages",
    "ghpages": "npm run preghpages && ghpages -p gh-pages",
    "gh-pages": "npm run ghpages",
    "start": "webpack-dev-server --content-base src --progress --watch --history-api-fallback --inline --hot",
    "deploy": "rm -rf build && npm run build && cp index.html build/index.html && cp -a src/assets build",
    "deploymin": "rm -rf build && npm run build && gulp && cp src/index.html build/index.html && cp -a src/assets build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/csepreghy/VR-Cinema-Website-with-React.js"
  },
  "keywords": [
    "aframe",
    "react",
    "vr",
    "a-frame",
    "mozvr",
    "webvr",
    "cinema",
    "booking"
  ],
  "author": "Andras Csepreghy <andris788@gmail.com>",
  "license": "Skyrel",
  "homepage": "",
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "css-loader": "^0.23.0",
    "eslint-plugin-react": "^5.0.1",
    "gulp-uglify": "^1.5.3",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
